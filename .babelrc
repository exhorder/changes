{
  "presets": [
    "react",
    [
      "latest",
      {
        "es2015": {
          "modules": false
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Array"
        ]
      }
    ],
    "idx",
    "lodash"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "latest"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        [
          "babel-plugin-transform-builtin-extend",
          {
            "globals": [
              "Array"
            ]
          }
        ],
        "idx"
      ]
    }
  }
}
